{
	"name": "pozitronik/yii2-options",
	"description": "Server-side options storage",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"extension"
	],
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master",
		"codeception/codeception": "^5.0",
		"codeception/module-asserts": "*",
		"codeception/module-yii2": "*",
		"codeception/module-db": "*",
		"vlucas/phpdotenv": "^5.4.1",
		"codeception/module-phpbrowser": "*"
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\sys_options\\": "src"
		}
	},
	"config": {
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}